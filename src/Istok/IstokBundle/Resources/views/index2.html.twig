<div class="row tile_count">
            <div class="col-md-3 col-sm-4 col-xs-6 tile_stats_count" style="border-width:0px">
              <span class="count_top"><i class="fa fa-user"></i> C.A aujourd'hui</span>
              <div class="count">{{params[0]}} Dhs</div>
            </div>
            <div class="col-md-3 col-sm-4 col-xs-6 tile_stats_count">
              <span class="count_top"><i class="fa fa-clock-o"></i> Nbr Commande</span>
              <div class="count">{{params[1]}}</div>
            </div>
            <div class="col-md-3 col-sm-4 col-xs-6 tile_stats_count">
              <span class="count_top"><i class="fa fa-user"></i> Nbr Article Aujourd'hui</span>
              <div class="count green">{{params[2]}}</div>
            </div>
            <div class="col-md-3 col-sm-4 col-xs-6 tile_stats_count">
              <span class="count_top"><i class="fa fa-user"></i> Nbr demande</span>
              <div class="count">{{params[3]}}</div>
            </div>
     
          </div>


</div>
</div>




<div class="col-md-12">


<div class="panel panel-primary">
          <div class="panel-heading">
            <h3 class="panel-title"><strong>Derniere commande effectuee</strong></h3>
          </div>
          <div class="panel-body">
            <div class="table-responsive">
           





<table class="table table-bordered search-table" >
<thead> 
<tr>



<th>num</th>
<th>client</th>
<th>date</th>
<th>total</th>
<th>avance</th>
<th>reste</th>

<th>facilite</th>

<th>remarque</th>


</tr>
</thead> 

<tbody>
{% set i=0 %}
{% for vente in ventes|reverse %}

{% if i<5 %}



<tr>
<td>{{vente.num}}</td>
<td>{{vente.client.prenom }} {{vente.client.nom }}</td>
<td>{{vente.date|date("m/d/Y h:m:s")}}</td>
<td>{{vente.total}} Dhs</td>


{% if vente.isavance==1 %}

<td>{{vente.avance}} Dhs</td>

{% else %}

<td>--</td>

{% endif %}


{% if vente.isavance==1 %}

<td>{{vente.total- vente.avance}} Dhs</td>

{% else %}

<td>--</td>

{% endif %}


{% if vente.isavance==1 %}

<td>{{vente.facilite.facilite}}</td>

{% else %}

<td>--</td>

{% endif %}



<td>{{vente.remarque}}</td>


</tr>


{% endif %}

  {% set i=i+1 %}

{% endfor%}



</tbody>

</table>




            </div>
          </div>

</div>
</div>











</div>




<div class="col-md-3">

<div class="panel panel-default">
  <div class="panel-body">
<div class="row tile_count">
            <div class="col-md-12 col-sm-12 col-xs-12 tile_stats_count" style="border-width:0px">
              <span class="count_top"><i class="fa fa-user"></i> Totale C.A</span>
              <div class="count" style="color:#18bc9c">{{params[4]}} Dhs</div>
              <span class="count_bottom"><i class="green">4% </i> From last Year</span>
              <hr>
              <span class="count_top"><i class="fa fa-user"></i> Montant credit </span>
              <div class="count" style="color:#E91E63">{{params[5]}} Dhs</div>
                                        <hr>
              <span class="count_top"><i class="fa fa-user"></i> Le montant à la caisse </span>
              <div class="count" style="color:#00BCD4">{{params[6]}} Dhs</div>

            </div>

     
          </div>


</div>
</div>


<div class="panel panel-default">
